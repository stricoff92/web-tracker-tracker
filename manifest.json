{
    "name": "Web Tracker Tracker",
    "description": "track web trackers",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
      "service_worker": "src/background.js"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "permissions": [
        "webRequest"
    ],
    "content_scripts": [],
    "action": {
      "default_popup": "src/popup/popup.html"
    }
  }